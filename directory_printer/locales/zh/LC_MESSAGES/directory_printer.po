msgid "Directory Printer v{}"
msgstr "目录打印工具 v{}"

msgid "Select Directory"
msgstr "选择目录"

msgid "Select ignore file (Optional)"
msgstr "选择忽略文件（可选）"

msgid "Browse"
msgstr "浏览"

msgid "Clear"
msgstr "清除"

msgid "Generate Directory Structure"
msgstr "生成目录结构"

msgid "Reset All"
msgstr "重置所有"

msgid "Stop"
msgstr "停止"

msgid "Copy to Clipboard"
msgstr "复制到剪贴板"

msgid "Download"
msgstr "下载"

msgid "Processing: {}/{} entries ({}%)"
msgstr "处理中：{}/{} 个条目 ({}%)"

msgid "Please select a directory first!"
msgstr "请先选择一个目录！"

msgid "Failed to process directory: {}"
msgstr "处理目录失败：{}"

msgid "Content copied to clipboard!"
msgstr "内容已复制到剪贴板！"

msgid "No content to copy!"
msgstr "没有可复制的内容！"

msgid "No content to download!"
msgstr "没有可下载的内容！"

msgid "File saved successfully!"
msgstr "文件保存成功！"

msgid "Failed to save file: {}"
msgstr "保存文件失败：{}"

msgid "Warning"
msgstr "警告"

msgid "Error"
msgstr "错误"

msgid "Success"
msgstr "成功"

msgid "Stop Generation?"
msgstr "停止生成？"

msgid "Quit Application?"
msgstr "退出应用程序？"

msgid "Do you want to stop?\n\n• Yes: Stop and clear output\n• No: Continue processing"
msgstr "您要停止吗？\n\n• 是：停止并清除输出\n• 否：继续处理"

msgid "Processing is in progress.\nDo you want to quit?\n\n• Yes: Stop processing and quit\n• No: Continue processing"
msgstr "正在处理中。\n您要退出吗？\n\n• 是：停止处理并退出\n• 否：继续处理"

msgid "[Permission Denied]"
msgstr "[权限被拒绝]"

msgid "Error: Directory '{}' not found!"
msgstr "错误：未找到目录 '{}'！" 